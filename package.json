        "flow-bin": "0.87.0",
        "flux-standard-action": "2.0.3",
        "gitbook-cli": "2.3.2",
        "jest": "23.6.0",
        "nock": "10.0.4",
        "redux-mock-store": "1.5.3",
        "redux-thunk": "2.3.0",
        "watch": "1.0.2",
        "webpack": "4.27.1",
        "webpack-cli": "3.1.2"
    }
