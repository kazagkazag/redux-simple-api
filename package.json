{
    "name": "redux-simple-api",
    "version": "0.3.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest ./src",
        "dev": "watch \"npm run flow status && npm run lint && npm run test\" ./src",
        "build:dev": "webpack --config webpack.config.dev.js",
        "build": "webpack --config webpack.config.prod.js -p",
        "lint": "eslint ./src",
        "flow": "flow",
        "docs:build": "rm -rf ./docs/* && gitbook build ./docs-src && mv ./docs-src/_book/* ./docs",
        "docs:dev": "gitbook serve ./docs-src",
        "prepublish": "npm run lint && npm run test && npm run docs:build && npm run build"
    },
    "author": "Kamil Zagrabski",
    "license": "ISC",
    "types": "./index.d.ts",
    "repository": "kazagkazag/redux-simple-api",
